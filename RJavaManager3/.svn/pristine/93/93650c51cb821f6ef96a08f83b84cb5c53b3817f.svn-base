							String outDescStat2 = "capture.output(outDesc,file=\"" + outFileName + "\",append = TRUE)"; 
	
	  						String runSuccessDescStat = rEngine.eval("outDesc").asString();
							if (runSuccessDescStat != null && runSuccessDescStat.equals("notRun")) {	
