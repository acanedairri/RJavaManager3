				 funcMergeDataResidR = "data_resid <- merge(dataMeaOneStage,meaOne2res_r,by=c(\"" + environment + "\", \"" + genotype + "\", \"" + block + "\", \"" + rep + "\", \"" + respvar[0] + "\"))";
