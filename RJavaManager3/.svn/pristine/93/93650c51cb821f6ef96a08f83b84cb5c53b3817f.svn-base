			//using the predicted means data
			if (isInputRawData == false) {
				dire3 = "dataMeans<- tryCatch(read.csv(\"" + dataFileName + "\", header = TRUE, na.strings = c(\"NA\",\".\"), blank.lines.skip=TRUE, sep = \",\"), error=function(err) \"notRun\")";				
