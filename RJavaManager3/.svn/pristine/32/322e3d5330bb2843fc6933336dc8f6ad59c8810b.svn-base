package org.irri.breedingtool.rjava.pbtoolsanalysis;

import org.irri.breedingtool.rjava.manager.RJavaManager;

public class TestNCII {

	private static String BSLASH = "\\";
	private static String FSLASH = "/";
	private static String PATH1 = System.getProperty("user.dir")+ System.getProperty("file.separator") + "sample_datasets" + System.getProperty("file.separator");
	public static String DATA_PATH = PATH1.replace(BSLASH, FSLASH);
	
	public static void main(String[] args) {
		
		//supply path and name of active data
		String dataFileName = DATA_PATH + "NCII_ME.csv";
		//supply path and name of text file where text output is going to be saved
		String outFileName = DATA_PATH + "NCII_output.txt";
		
		//specify parameters
		String design = "RCB";
		String[] respvars = {"Y"};
		String female = "Female";
		String male = "Male";
		String rep = "NULL";
		String block = "Block";
		String inbred = "TRUE";
		String environment = "Env";
		String individual = "NULL";
		
		RJavaManager rJavaManager= new RJavaManager();
		rJavaManager.initPBtool();
		rJavaManager.getPbToolAnalysisManager().doNC2(dataFileName, outFileName, design, respvars, female, male, rep, block, inbred, individual, environment);
	}

}